    "homepage": "http://konkinoani.me/",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-authclient": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
